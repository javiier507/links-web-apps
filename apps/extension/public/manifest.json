{
  "manifest_version": 3,
  "name": "Links Extension",
  "version": "1.0.0",
  "description": "Gets the current tab URL",
  "action": {
    "default_popup": "index.html",
    "default_title": "Links Extension"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "icons": {
    "16": "vite.svg",
    "48": "vite.svg",
    "128": "vite.svg"
  }
}
